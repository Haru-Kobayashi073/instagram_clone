<%= form_with(model: article) do |form| %>
  <%#= error_messages(article) %>

  <div>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>

  <div>
    <%= form.label :thumbnail %>
    <%= form.file_field :thumbnail %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>